{
  "name": "@folio/plugin-eusage-reports",
  "version": "1.2.0",
  "description": "Stripes plugin module to View eUsage reports",
  "repository": "folio-org/ui-plugin-eusage-reports",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "main": "src/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "stripes": {
    "actsAs": [
      "handler",
      "plugin"
    ],
    "handlerName": "eventHandler",
    "pluginType": "ui-agreements-extension",
    "displayName": "ui-plugin-eusage-reports.meta.title",
    "optionalOkapiInterfaces": {
      "eusage-reports": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.ui-plugin-eusage-reports.enabled",
        "displayName": "UI: eusage-reports plugin is enabled"
      }
    ]
  },
  "scripts": {
    "lint": "eslint -f unix .",
    "qlint": "eslint -f unix  --rule '{\"no-console\":\"off\"}' .",
    "test": "jest",
    "start": "stripes serve --port 3005"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^5.4.0",
    "@folio/stripes": "^6.0.0 || ^7.0.0",
    "@folio/stripes-cli": "^2.4.0",
    "@folio/stripes-components": "^9.0.0 || ^10.0.0",
    "@folio/stripes-core": "^7.0.0 || ^8.0.0",
    "@testing-library/dom": "^7.26.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.1.10",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.3",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.0",
    "jest-junit": "^12.0.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-intl": "^5.8.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "@folio/stripes-webpack": "^1.2.0",
    "@folio/stripes-acq-components": "^2.4.3",
    "chart.js": "^3.1.0",
    "lodash": "^4.17.21",
    "randomcolor": "^0.6.2",
    "react-chartjs-2": "^2.11.1"
  },
  "optionalDependencies": {
    "@folio/plugin-find-eresource": "^2.1.0 || ^3.0.0 || ^4.0.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^6.0.0 || ^7.0.0",
    "prop-types": "*",
    "react": "^16.0.0 || ^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.0",
    "react-router-dom": "^5.2.0"
  }
}
